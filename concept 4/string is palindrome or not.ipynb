    "#given a string input, find out if its a palindrome\n",
    "def func_str(str1):\n",
    "    str2=str1[::-1]#it reverse the order of word \n",
    "    if str2==str1:\n",
    "        return 'its is palindrome'\n",
    "    else:\n",
    "        return 'it is not a palindrome'\n",
    "    \n",
    "str3=\"naman\"\n",
    "func_str(str3)\n",
    "\n"
